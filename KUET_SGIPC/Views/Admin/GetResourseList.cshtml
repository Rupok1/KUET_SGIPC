@model IEnumerable<KUET_SGIPC.Models.ResourseTable>

@{
    ViewBag.Title = "GetResourseList";
}

<h2>GetResourseList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Link)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Link)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditResourse", new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteResourse", new { id=item.Id })
        </td>
    </tr>
}

</table>
